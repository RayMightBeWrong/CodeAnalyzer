digraph {
	0 [label="entry foo" shape=trapezium]
	1 [label=if shape=diamond]
	0 -> 1
	2 [label=then shape=diamond]
	1 -> 2
	3 [label="func:write"]
	2 -> 3
	4 [label=elif shape=diamond]
	1 -> 4
	5 [label="func:write"]
	4 -> 5
	6 [label=if shape=diamond]
	0 -> 6
	7 [label=then shape=diamond]
	6 -> 7
	8 [label="func:write"]
	7 -> 8
	9 [label=else shape=diamond]
	6 -> 9
	10 [label="func:write"]
	9 -> 10
	11 [label=return]
	0 -> 11
}
