digraph {
	0 [label=begin]
	-2 [label=end]
	1 [label="decl: i"]
	0 -> 1
	2 [label=switch shape=diamond]
	1 -> 2
	3 [label=case shape=diamond]
	2 -> 3
	4 [label="func:write"]
	3 -> 4
	5 [label=return]
	4 -> 5
	6 [label=case shape=diamond]
	2 -> 6
	7 [label="func:write"]
	6 -> 7
	8 [label=return]
	7 -> 8
	9 [label=default shape=diamond]
	2 -> 9
	10 [label="func:write"]
	9 -> 10
	11 [label=return]
	10 -> 11
	5 -> -2
	8 -> -2
	11 -> -2
}
